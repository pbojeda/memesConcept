# Express Server Port
PORT=4000

# MongoDB Connection String
MONGODB_URI=mongodb+srv://user:pass@cluster.mongodb.net/dbname

# Stripe Secret Key (starts with sk_test_...)
STRIPE_SECRET_KEY=sk_test_...

# Cloudinary (for Image Uploads)
CLOUDINARY_CLOUD_NAME=...
CLOUDINARY_API_KEY=...
CLOUDINARY_API_SECRET=...

# Admin API Key (must match frontend NEXT_PUBLIC_ADMIN_API_KEY)
ADMIN_API_KEY=your-secret-key

# Printful API Key and optionally Store ID
PRINTFUL_API_KEY=your-printful-token
PRINTFUL_STORE_ID=your-store-id
